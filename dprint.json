{
  "incremental": true,
  "lineWidth": 80,
  "useTabs": false,
  "indentWidth": 2,
  "typescript": {
  },
  "json": {
  },
  "markdown": {
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "**/.next",
    "**/coverage"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.60.0.wasm",
    "https://plugins.dprint.dev/json-0.13.2.wasm",
    "https://plugins.dprint.dev/markdown-0.11.3.wasm"
  ]
}
